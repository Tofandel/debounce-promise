{
  "name": "@tofandel/debounce-promise",
  "version": "4.1.3",
  "description": "Create a debounced version of a promise returning function",
  "main": "dist/index",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "tap --100 && npm run lint",
    "lint": "standard | snazzy",
    "lint:fix": "standard --fix | snazzy",
    "compile": "babel index.js --source-maps --out-dir dist",
    "clean": "rimraf dist",
    "prepare": "in-publish && npm run compile || not-in-publish",
    "postpublish": "in-publish && npm run clean || not-in-publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tofandel/debounce-promise.git"
  },
  "keywords": [
    "promise",
    "batch",
    "accumulate",
    "debounce",
    "throttle",
    "ratelimit"
  ],
  "author": "Bjørge Næss",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tofandel/debounce-promise/issues"
  },
  "homepage": "https://github.com/tofandel/debounce-promise",
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "eslint": "^8.49.0",
    "in-publish": "^2.0.1",
    "rimraf": "^5.0.1",
    "snazzy": "^9.0.0",
    "standard": "^17.1.0",
    "tap": "^16.3.8"
  }
}
